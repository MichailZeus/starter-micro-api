асия)","type":"text","version":1}],"direction":"ltr","format":"","indent":1,"type":"listitem","version":1,"value":5},{"children":[{"detail":0,"format":0,"mode":"normal","style":"","text":"Использование дополнительных аккаунтов для обхода ","type":"text","version":1},{"detail":0,"format":1,"mode":"normal","style":"","text":"ограничений","type":"text","version":1}],"direction":"ltr","format":"","indent":1,"type":"listitem","version":1,"value":6},{"children":[{"detail":0,"format":0,"mode":"normal","style":"","text":"Нарушение ","type":"text","version":1},{"children":[{"detail":0,"format":0,"mode":"normal","style":"","text":"правил сервера","type":"text","version":1}],"direction":"ltr","format":"","indent":0,"type":"custom-link","version":1,"rel":null,"target":null,"title":null,"url":"https://pepeland.net/rules"},{"detail":0,"format":0,"mode":"normal","style":"","text":", ","type":"text","version":1},{"children":[{"detail":0,"format":0,"mode":"normal","style":"","text":"пользовательского соглашения","type":"text","version":1}],"direction":"ltr","format":"","indent":0,"type":"custom-link","version":1,"rel":null,"target":null,"title":null,"url":"https://pepeland.net/wiki/legal/terms-of-use"},{"detail":0,"format":0,"mode":"normal","style":"","text":", правил (","type":"text","version":1},{"children":[{"detail":0,"format":0,"mode":"normal","style":"","text":"Terms of Service","type":"text","version":1}],"direction":"ltr","format":"","indent":0,"type":"custom-link","version":1,"rel":"noopener noreferrer","target":"_blank","title":null,"url":"https://twitch.tv/p/en/legal/terms-of-service/"},{"detail":0,"format":0,"mode":"normal","style":"","text":") платформы Twitch.tv","type":"text","version":1}],"direction":"ltr","format":"","indent":1,"type":"listitem","version":1,"value":7}],"direction":"ltr","format":"","indent":0,"type":"list","version":1,"listType":"bullet","start":1,"tag":"ul"}],"direction":"ltr","format":"","indent":0,"type":"listitem","version":1,"value":4}],"direct